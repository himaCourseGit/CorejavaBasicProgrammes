class Author 
{
	public String names;
	public String title;
	public int pages;
	public static String genre="English";
	public Author(String names,String title,int pages)
	{
		this.names=names;
		this.title=title;
		this.pages=pages;
	}
	public void displayAuthorDetails()
		{
		System.out.println("names"+names);
		System.out.println("title"+title);
		System.out.println("pages"+pages);
		System.out.println("genre"+genre);
		}
		
	public static void main(String[] args) 
	{
		Author author1=new Author("bindu","civils",200);
		Author author2=new Author("chitti","embedd",100);
		author1.displayAuthorDetails();
		author2.displayAuthorDetails();
	}
}
